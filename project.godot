config_version=5

[application]

config/name="Torah Codes"
run/main_scene="res://scenes/authentication.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=25
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

Menu="*res://scenes/menu.tscn"
Global="*res://scripts/global.gd"
MaterialIconsDB="*res://addons/material-design-icons/icons/icons.gd"
Firebase="*res://addons/FirebaseAPI/firebase.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/FirebaseAPI/plugin.cfg", "res://addons/Page_plugin/plugin.cfg", "res://addons/material-design-icons/plugin.cfg")

